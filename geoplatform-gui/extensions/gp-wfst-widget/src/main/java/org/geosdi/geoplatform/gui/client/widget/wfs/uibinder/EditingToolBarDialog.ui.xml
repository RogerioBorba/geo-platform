<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:wfsEdit='urn:org.geosdi.geoplatform.gui.client.widget.wfs.uibinder'>

    <ui:style>
        .panel {
        position: absolute;
        top: 70px;
        padding: 0 12px;
        width: 50px;
        height: 300px!important;
        overflow-x: auto;
        z-index: 1000!important;
        }
    </ui:style>

    
    <g:FlowPanel styleName="{style.panel}">
        <g:ToggleButton ui:field="info" />
        <g:ToggleButton ui:field="edit"/>
        <g:ToggleButton ui:field="reshape"/>
        <g:ToggleButton ui:field="rotate" />
        <g:ToggleButton ui:field="drag" />
        <g:ToggleButton ui:field="resize"/>
    </g:FlowPanel>
    
</ui:UiBinder>
